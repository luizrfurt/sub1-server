{
   "name": "sub1-server",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
      "build": "tsc -p .",
      "typeorm": "typeorm-ts-node-commonjs",
      "db:migrate": "rmdir /s /q build && yarn build && yarn typeorm migration:generate ./src/migrations/added-migration -d ./src/utils/dataSource.ts",
      "db:ddl": "rmdir /s /q build && yarn build && yarn typeorm migration:run -d src/utils/dataSource.ts",
      "db:seed": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/utils/dataSource.ts",
      "db:push": "yarn db:migrate && yarn db:ddl && yarn db:seed"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.6",
      "@types/config": "^3.3.3",
      "@types/cookie-parser": "^1.4.6",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/html-to-text": "^9.0.4",
      "@types/moment-timezone": "^0.5.30",
      "@types/morgan": "^1.9.9",
      "@types/node": "^16.18.68",
      "@types/nodemailer": "^6.4.14",
      "@types/pug": "^2.0.10",
      "morgan": "^1.10.0",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/jsonwebtoken": "^9.0.5",
      "aws-sdk": "^2.1546.0",
      "axios": "^1.6.2",
      "bcryptjs": "^2.4.3",
      "config": "^3.3.9",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "cpf-cnpj-validator": "^1.0.3",
      "dotenv": "^16.3.1",
      "envalid": "^8.0.0",
      "express": "^4.17.2",
      "html-to-text": "^9.0.5",
      "jsonwebtoken": "8.5.1",
      "nodemailer": "^6.9.7",
      "pg": "^8.4.0",
      "pug": "^3.0.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17",
      "typeorm-extension": "^3.5.0",
      "zod": "^3.22.4"
   }
}
